<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorSelectionSectionView">
        <attr name="android:title" />
        <attr name="index" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="android:title" />
        <attr name="android:icon" />
        <attr name="android:textColor" />
        <attr name="backgroundColor" />
    </declare-styleable>

    <declare-styleable name="ActionToolbar">
        <attr name="android:title" format="string" />
        <attr name="leftMenuTitle" format="string" />
        <attr name="rightMenuTitle" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomToolbar">
        <attr name="titleToolbar" format="string"/>
        <attr name="showBackPressButton" format="boolean"/>
        <attr name="iconRight" format="reference"/>
        <attr name="iconLeft" format="reference"/>
        <attr name="textDone" format="string"/>
    </declare-styleable>

</resources>
